Documents all of the Buttons and Joysticks of the controllers!

enum:
    XboxA(1),
    XboxB(2),
    XboxX(3),
    XboxY(4),
    XboxLEFTBumper(5),
    XboxRIGHTBumper(6),
    XboxBACK(7),
    XboxSTART(8),
    XboxLEFTSTICKX(-5),
    XboxLEFTSTICKY(-6),
    XboxRIGHTSTICKX(-7),
    XboxRIGHTSTICKY(-8),
    XboxLEFTSTICKBUTTON(9),
    XboxRIGHTSTICKBUTTON(10),
    XboxRIGHTTrigger(3),
    XboxLEFTTrigger(2);
